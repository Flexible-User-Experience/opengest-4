{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": ">=8.3",
        "ext-calendar": "*",
        "ext-ctype": "*",
        "ext-fileinfo": "*",
        "ext-iconv": "*",
        "ext-zip": "*",
        "ext-gd": "*",
        "ext-mysqli": "*",
        "ext-intl": "*",
        "beberlei/doctrineextensions": "^1.0",
        "beelab/recaptcha2-bundle": "^2.3",
        "doctrine/annotations": "^1.0",
        "doctrine/doctrine-bundle": "^2.3",
        "doctrine/doctrine-migrations-bundle": "^3.1",
        "doctrine/orm": "^2.7",
        "egulias/email-validator": "^2.1",
        "friendsofsymfony/ckeditor-bundle": "^2.1",
        "friendsofsymfony/jsrouting-bundle": "^3.0",
        "knplabs/knp-paginator-bundle": "^5.0",
        "liip/imagine-bundle": "^2.1",
        "mirmit/e-factura-bundle": "v0.0.14-alpha",
        "phpdocumentor/reflection-docblock": "^5.3",
        "phpoffice/phpspreadsheet": "^1.23",
        "presta/sitemap-bundle": "^4.0",
        "qipsius/tcpdf-bundle": "^2.0",
        "sensio/framework-extra-bundle": "^6.2",
        "setasign/fpdi": "^2.0",
        "setasign/fpdi_pdf-parser": "^2.1",
        "sonata-project/admin-bundle": "^4.12",
        "sonata-project/doctrine-orm-admin-bundle": "^4.0",
        "sonata-project/exporter": "^2.9",
        "stof/doctrine-extensions-bundle": "^1.3",
        "symfony/asset": "6.0.*",
        "symfony/console": "6.0.*",
        "symfony/dotenv": "6.0.*",
        "symfony/expression-language": "6.0.*",
        "symfony/flex": "^2.2",
        "symfony/form": "6.0.*",
        "symfony/framework-bundle": "6.0.*",
        "symfony/http-client": "6.0.*",
        "symfony/intl": "6.0.*",
        "symfony/mailer": "6.0.*",
        "symfony/monolog-bundle": "^3.1",
        "symfony/process": "6.0.*",
        "symfony/property-access": "6.0.*",
        "symfony/property-info": "6.0.*",
        "symfony/proxy-manager-bridge": "6.0.*",
        "symfony/runtime": "6.0.*",
        "symfony/security-bundle": "6.0.*",
        "symfony/serializer": "6.0.*",
        "symfony/string": "^5.3",
        "symfony/translation": "6.0.*",
        "symfony/twig-bundle": "6.0.*",
        "symfony/ux-twig-component": "^2.12",
        "symfony/validator": "6.0.*",
        "symfony/web-link": "6.0.*",
        "symfony/webpack-encore-bundle": "^1.2",
        "symfony/yaml": "6.0.*",
        "twig/extensions": "^1.5",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/twig": "^2.0",
        "vich/uploader-bundle": "^2.0",
        "xsolve-pl/xsolve-cookie-acknowledgement-bundle": "^1.0"
    },
    "require-dev": {
        "ext-dom": "*",
        "ext-sqlite3": "*",
        "hautelook/alice-bundle": "^2.0",
        "phpunit/phpunit": "9.5",
        "symfony/browser-kit": "^6.0",
        "symfony/css-selector": "^6.0",
        "symfony/debug-bundle": "^6.0",
        "symfony/maker-bundle": "^1.0",
        "symfony/phpunit-bridge": "^6.0",
        "symfony/stopwatch": "^6.0",
        "symfony/web-profiler-bundle": "^6.0",
        "thibautselingue/local-php-security-checker-installer": "^1.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "symfony/flex": true,
            "setasign/composer-ioncube-license-installer": true,
            "symfony/runtime": true,
            "composer/installers": true,
            "oomphinc/composer-installers-extender": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "ckeditor:install --clear=drop": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts",
            "local-php-security-checker-installer && local-php-security-checker"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "6.0.*"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url":  "https://github.com/kgilden/twig-spreadsheet-bundle.git"
        },
        {
            "type": "composer",
            "url": "https://www.setasign.com/downloads/"
        },
        {
            "type": "vcs",
            "url": "https://github.com/Mirmit/EFacturaBundle.git"
        }
    ]
}
