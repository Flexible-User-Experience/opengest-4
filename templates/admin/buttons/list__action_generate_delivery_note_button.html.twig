{% if admin.isGranted('EDIT', object) and admin.hasRoute('edit') %}
    <a href="{{ admin.generateObjectUrl('generateDeliveryNoteFromSaleRequest', object) }}"
       class="btn btn-sm btn-default edit_link{% if object.saleRequestHasDeliveryNotes|length > 0 %} disabled {% endif %}"
       title="Generate Delivery Note">
        <i class="fa fa-file-archive-o"></i>
    </a>
{% endif %}
