{% extends 'SonataAdminBundle:CRUD:base_list_field.html.twig' %}

{% block field %}
    {% spaceless %}
        {% if object.profilePhotoImage %}
            <img src="{{ vich_uploader_asset(object, 'profilePhotoImageFile') | imagine_filter('60x60') }}" alt="thumbnail">
        {% else %}
            <img src="https://via.placeholder.com/60x60.png?text=?" alt="thumbnail">
        {% endif %}
    {% endspaceless %}
{% endblock %}
